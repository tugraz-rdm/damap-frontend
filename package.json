    "build-openshift": "ng build damap-frontend --configuration=openshift",
    "build-prod": "ng build damap-frontend --configuration=production",
    "watch": "ng build damap-frontend --watch --configuration development",
    "test": "ng test damap-frontend --browsers=ChromeHeadless --watch=false",
