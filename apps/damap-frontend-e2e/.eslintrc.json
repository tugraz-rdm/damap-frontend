{
  "extends": ["plugin:cypress/recommended"],
  "ignorePatterns": ["!**/*"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "cypress"],
  "overrides": [
    {
      "files": ["*.ts"],
      "rules": {
        "@typescript-eslint/no-namespace": "off",
        "@typescript-eslint/no-unused-vars": "off"
      }
    }
  ]
}
