{
  "affected": {
    "defaultBase": "next"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "e2e": {
      "cache": true
    }
  },
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "generators": {
    "@nx/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "karma",
      "e2eTestRunner": "none"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "karma"
    },
    "@nx/angular:component": {
      "style": "css"
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}//*", "sharedGlobals"],
    "sharedGlobals": [],
    "production": ["default"]
  }
}
